{
    "name": "slymbo/laraposts",
    "description": "Post package",
    "homepage": "https://github.com/slymbo/laraposts",
    "license": "MIT",
    "version": "0.1",
    "authors": [
        {
            "name": "Slymbo"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Slymbo\\Laraposts\\": "src/"
        },
        "psr-0": {
            "Slymbo\\Laraposts\\": "src/"
        }
    },
    "extra": {
        "providers": [
            "Slymbo\\Laraposts\\Providers\\LarapostsServiceProvider"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Slymbo\\Laraposts\\": "tests/"
        },
        "psr-0": {
            "Slymbo\\Laraposts\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "../../../vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../packages/slymbo/laraposts",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
